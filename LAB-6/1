#include <stdio.h>
#include <stdlib.h>
#include<string.h>
void swap(int* x,int* y)
{
    int temp = *x;
    *x = *y;
    *y = temp;
}
int main()
{
    int n,a[100],A[100][100];
    memset(a,0,sizeof(a));
    printf("Enter the value of n : ");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
        scanf("%d",&a[i]);
    printf("Enter nxn array \n");
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++)
        {
            scanf("%d",&A[i][j]);
            if(A[i][j]==1)
            {
                swap(a+i,a+j);
            }
        }
    }
    for(int i=0;i<n;i++)
        printf("%d ",a[i]);
}

