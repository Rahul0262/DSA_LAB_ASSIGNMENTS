#include <stdio.h>
#include <stdlib.h>
void swap(int* x,int *y)
{
    int temp = *x;
    *x = *y;
    *y = temp;
}
int main()
{
    int n,cnt=0;;
    int array[n];
    printf("Enter the value of n : ");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&array[i]);
    }
    for(int i = n-1; i > 0;i--)
    {
        for(int j=0;j<i-1;j++)
        {
            if(array[j]>array[i])
            {
                swap(&array[i],&array[j]);
                for(int i = 0;i<n;i++)
                {
                    printf("%d ",array[i]);
                }
                printf("\n");
                cnt++;
            }
        }
    }
    printf("Cnt : %d",cnt);
}
